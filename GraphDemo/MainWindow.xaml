<Window x:Class="GraphDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Image Height="150" HorizontalAlignment="Left" Name="graphImage" Stretch="Fill" VerticalAlignment="Top" Width="503">
            
        </Image>

        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="0,167,0,0" Name="plotButton" VerticalAlignment="Top" Width="75" Click="plotButton_click" />
        <Label Content="Label" Height="28" HorizontalAlignment="Left" Margin="0,283,0,0" Name="duration" VerticalAlignment="Top" Width="511" />
        <Button Content="Clear" Height="23" HorizontalAlignment="Left" Margin="187,167,0,0" Name="btnClear" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <TextBox Height="81" HorizontalAlignment="Left" Margin="0,196,0,0" Name="textBox1" VerticalAlignment="Top" Width="503" TextWrapping="WrapWithOverflow" />
    </Grid>
</Window>
